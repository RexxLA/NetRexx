class serialize_tests01 extends Object

method sertest() static

   a = Rexx 'abase'
   b = Rexx 1

   a[0]=5
   a[1]=11
   a[2]=222
   a[3]=3333
   a[4]=444
   a[5]=55

   pipe (serpipe1 debug 0 ) stem a | serialize | rexx insert /aYzB/ after | diskw stream a.rex
   pipe (serpipe2 debug 0 ) diskr stream a.rex | deblock string /aYzB/ | serialize rexx | stem b

   o = ''
   loop i=1 to b[0]
 --     say 'b['i']='b[i]
      o = o b[i]
   end

   if o = "11 222 3333 444 55" then
     say "#1 Serialize *OK*"
   else
     say "#1 Serialize **FAIL**"


method main(a=String[]) static

   sertest()
   exit


