package org.netrexx.njpipes.stages

import org.netrexx.njpipes.pipes.

class dicta extends stage

method run() public

   a = dictionary arg(null)

   loop forever
      i = readto()
      a.put(i,readto())
   catch StageError
      rc = rc()
   end

   exit(rc*(rc<>12))

