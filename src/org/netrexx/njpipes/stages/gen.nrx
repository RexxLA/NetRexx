options nostrictcase nostrictargs nostrictsignal
package org.netrexx.njpipes.stages
import org.netrexx.njpipes.pipes.

class gen extends stage
  
  method run() public
    
    n = arg()
    if \n.datatype('W') then
      exit(16)
    i=Rexx
    loop i=1 to n
      output(i' ')
    catch StageError
      rc = rc()
    end
    
    exit(rc)
    