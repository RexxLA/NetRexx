options nostrictcase nostrictargs nostrictsignal
package org.netrexx.njpipes.stages
import org.netrexx.njpipes.pipes.


class xconsole extends stage

method run() public

   a = arg()

   do
      loop forever
         line = Rexx peekto()
         say a' 'line
         output(line)
         readto()
      end
   catch StageError
      rc = rc()
   end

   exit(rc*(rc<>12))

