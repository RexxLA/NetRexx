pipe (sqlselect_tests01)

 o: faninany |
    sort singleok |
    specs 2-* 1 |
    cons ?

-- sqlselect (from stream) ?
   literal * from FlightRoute where flight = 'KLM765'; |
   sqlselect |
   c1: compare any ~2#1 sqlselect (from stream) ~ equal ~*OK*~ notequal ~**FAIL**  at rec \\c col \\b.~ less /Less:\nActual:\n\\p\nExpected:\n\\s\n/ more /More:\nActual:\n\\p\nExpected:\n\\s\n/ |
   o: ?
   literal FLIGHT--ROUTE--UPDATETIME-- ; KLM765  AUA-BON-AMS  1494132448 |
   split ; |
   c1: ?

